<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.HBox?>

<fx:root style="-fx-border-color: black;" type="GridPane" xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1">
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints fillHeight="false" maxHeight="20.0" minHeight="20.0" prefHeight="20.0" vgrow="NEVER" />
    <RowConstraints fillHeight="false" maxHeight="20.0" minHeight="20.0" prefHeight="20.0" vgrow="NEVER" />
    <RowConstraints fillHeight="false" maxHeight="20.0" minHeight="20.0" prefHeight="20.0" vgrow="NEVER" />
      <RowConstraints vgrow="ALWAYS" />
  </rowConstraints>
   <children>
      <Label fx:id="joinParameterName" text="Join Parameter Name:" />
      <Label fx:id="waitForNJobs" text="Wait For N Jobs:" GridPane.rowIndex="1" />
      <Label fx:id="joinedJobIds" text="Joined Jobs:" GridPane.hgrow="ALWAYS" GridPane.rowIndex="2" />
      <HBox fx:id="buttonBar" GridPane.rowIndex="3" />
   </children>
   <padding>
      <Insets left="5.0" right="5.0" />
   </padding>
</fx:root>
